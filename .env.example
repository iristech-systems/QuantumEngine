# QuantumORM Environment Configuration Example
# Copy this file to .env and update with your actual values

# SurrealDB Configuration (Docker defaults)
SURREALDB_URL=ws://localhost:8000/rpc
SURREALDB_NS=test_ns
SURREALDB_DB=test_db
SURREALDB_USER=root
SURREALDB_PASS=root

# ClickHouse Configuration (Docker defaults)
CLICKHOUSE_URL=localhost
CLICKHOUSE_PORT=8123
CLICKHOUSE_USER=default
CLICKHOUSE_PASS=""
CLICKHOUSE_DB=default
CLICKHOUSE_SECURE=false

# Development Settings
DEBUG=true
LOG_LEVEL=INFO

# Testing Configuration
TEST_SURREALDB_URL=ws://localhost:8000/rpc
TEST_CLICKHOUSE_URL=localhost

# Production Settings (commented out for local development)
# SURREALDB_URL=wss://your-production-surrealdb.com/rpc
# CLICKHOUSE_URL=your-production-clickhouse.com
# CLICKHOUSE_PORT=443
# CLICKHOUSE_SECURE=true